        <tr>
          <td>BILL
            <a name="BILL"> </a>
          </td>
          <td>Billing</td>
          <td>Contact details for information regarding to billing/general finance enquiries.</td>
        </tr>
        <tr>
          <td>ADMIN
            <a name="ADMIN"> </a>
          </td>
          <td>Administrative</td>
          <td>Contact details for administrative enquiries.</td>
        </tr>
        <tr>
          <td>HR
            <a name="HR"> </a>
          </td>
          <td>Human Resource</td>
          <td>Contact details for issues related to Human Resources, such as staff matters, OH&amp;S etc.</td>
        </tr>
        <tr>
          <td>PAYOR
            <a name="PAYOR"> </a>
          </td>
          <td>Payor</td>
          <td>Contact details for dealing with issues related to insurance claims/adjudication/payment.</td>
        </tr>
        <tr>
          <td>PATINF
            <a name="PATINF"> </a>
          </td>
          <td>Patient</td>
          <td>Generic information contact for patients.</td>
        </tr>
        <tr>
          <td>PRESS
            <a name="PRESS"> </a>
          </td>
          <td>Press</td>
          <td>Dedicated contact point for matters relating to press enquiries.</td>
        </tr>
